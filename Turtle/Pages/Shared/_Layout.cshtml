<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Turtle</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/Turtle.styles.css" asp-append-version="true"/>
    <link rel="script" href="js/site.js" asp-append-version="true"/>
</head>

<body>
<nav class="sidebar close">
    <header>
        <div class="image-text">
            <span class="image">
                <img src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeCAYAAAA7MK6iAAAAAXNSR0IArs4c6QAAAI5JREFUSEvtVkEOgCAMKy/Tn6k/w5dpmjizDOWkjIQ14TIYg9IuJDghOdVFFG7G/BvVx8MJZO0KYDHzGwDGiVrunRaFhYrxqP5d3d01EFrDvjMtQ8wAJkPJDiBfsVpu2KnQ0nh2kr6rqZAYxcWhQWFpcVkKi/26s5NbAxlPXHFjiq3J18dN1VH4cwbcWuYJdlBCH81jE58AAAAASUVORK5CYII="/>
            </span>
            
            <div class="text header-text">
                <span class="name">Turle</span>
                <span class="slogan">By students to students</span>
            </div>
        </div>
        <span class="toggle">
        <img src="data:image/png;base64,"/>
        </span>
    </header>
    <hr class="hr"/>
    <div class="menu-bar">
        <div class="menu">
            <ul class="menu-links ps-0">
                <li>
                    <img src="data:image/png;base64,"/>    
                        <button  href="#" class="btn btn-link profile-button"><span class="text nav-text">My profile</span></button
                </li>
                <li class="animated-button">
                    <img src="data:image/png;base64,"/>
                    <button href="#" class="btn btn-link chats-button"><span class="text nav-text">Chats</span><span></span><span></span><span></span><span></span> </button>
                </li>
                
                <li class="link">
                    <a href="#">
                        <img src="data:image/png;base64,"/>
                        <span class="text nav-text">Search</span>
                    </a>
                </li>
                
                <li class="link">
                    <a href="#">
                        <img src="data:image/png;base64,"/>
                        <span class="text nav-text"> Schedule </span>
                    </a>
                </li>
                <li class="link position-absolute bottom-0 start-0" id="logout">
                    
                        <img src="data:image/png;base64,"/>
                        <button href="#" class=""<span class="text nav-text">Logout </span>
                   
                </li>
                
            </ul>
        </div>
    </div>
</nav>
<body>
<container>
    <div class="content">
        @RenderBody()
    </div>
</container>
</body>
<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2022 - Turtle - <a asp-area="" asp-page="/Privacy">Privacy</a>
    </div>
</footer>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>

@await RenderSectionAsync("Scripts", required: false)
</body>
</html>